# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Thyme-Saver' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Thyme-Saver

  target 'Thyme-SaverTests' do
    inherit! :search_paths
    # Pods for testing

	pod 'Firebase/Core'
	pod 'FirebaseUI'
	pod 'FirebaseUI/Google'
	pod 'FirebaseUI/Auth'
	pod 'Firebase/Auth'

  end

  target 'Thyme-SaverUITests' do
    inherit! :search_paths
    # Pods for testing
  end
	pod 'Firebase/Core'
	pod 'FirebaseUI'
	pod 'FirebaseUI/Google'
	pod 'FirebaseUI/Auth'
	pod 'Firebase/Auth'


end
